<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="top" namespace="/" extends="struts-default">
        <action name="TopAction" class="action.TopAction" method="execute">
            <result name="ok">/Login.jsp</result>
        </action>
        <action name="AuthAction" class="action.AuthAction" method="execute">
            <result name="found">/Top.jsp</result>
            <result name="error">/error.jsp</result>
        </action>
    </package>
    <constant name="struts.action.extension" value="" />
</struts>
